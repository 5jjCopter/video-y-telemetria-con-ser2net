sudo wget https://github.com/5jjCopter/video-y-telemetria-con-ser2net/archive/master.zip

sudo unzip master.zip

cd video-y-telemetria-con-ser2net-master

sudo chmod 775 istall-ser2net-video-telemetria

sudo chmod 775 istall-ser2net-video-telemetria-rpi2

sudo cp istall-ser2net-video-telemetria /home/pi

sudo cp istall-ser2net-video-telemetria-rpi2 /home/pi

cd

sudo rm master.zip

sudo rm -rf video-y-telemetria-con-ser2net-master

cd

sudo ./istall-ser2net-video-telemetria-rpi2

cd
